    <bean name="room" class="domain.Room">
        <constructor-arg name="array">
            <array value-type="java.lang.String">
                <value>1</value>
                <value>2</value>
                <value>3</value>
            </array>
        </constructor-arg>
        <constructor-arg name="computers">
            <array>
                <bean class="domain.Computer"></bean>
                <bean class="domain.Computer"></bean>
                <bean class="domain.Computer"></bean>
            </array>
        </constructor-arg>
        <constructor-arg name="students">
            <array>
                <bean class="domain.Student"></bean>
                <bean class="domain.Student"></bean>
                <bean class="domain.Student"></bean>
            </array>
        </constructor-arg>
    </bean>
    <bean name="computer" class="domain.Computer" c:brand="huawei" c:price="8000"></bean>


